{
    "name": "parfumix/nova-scheduled-jobs",
    "description": "A Laravel Nova card to view your scheduled jobs.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/llaski/nova-scheduled-jobs",
    "license": "MIT",
    "authors": [
        {
            "name": "Larry Laski",
            "email": "larry.laski@gmail.com",
            "homepage": "https://larrylaski.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "dragonmantank/cron-expression": "^2.2",
        "laravel/nova": "^1.0|^2.0|^3.0",
        "illuminate/console": "^5.6|^6.0"
    },
    "require-dev": {
        "illuminate/cache": "*",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Llaski\\NovaScheduledJobs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Llaski\\NovaScheduledJobs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Llaski\\NovaScheduledJobs\\NovaScheduledJobsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
